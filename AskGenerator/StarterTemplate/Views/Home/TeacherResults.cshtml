@model AskGenerator.Mvc.ViewModels.Teacher.TeacherDetailsViewModel
<div class="col-sm-offset-1 col-sm-10 detailsWrapper">
    <div class="col-xs-4 col-md-3 col-lg-2 thumbnail">
        <img src="@Model.Image" alt="@Model.GetShortName()" />
        <div class="caption">
            @Model.FullName()
        </div>
    </div>
    <div class="col-sm-8 col-md-9 col-lg-10">
        <table>
            <tbody>
                @foreach (var q in Model.Questions)
                {
                    var id = q.Key + 'm';
                    <tr>
                        <td>@q.Value</td>
                        <td id="@id"></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>